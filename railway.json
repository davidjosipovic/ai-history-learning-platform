{
  "builds": [
    {
      "builder": "DOCKERFILE",
      "dockerfilePath": "ai-rag-service/Dockerfile",
      "dockerContext": "ai-rag-service"
    }
  ],
  "deploy": {
    "startCommand": "uvicorn app.main:app --host 0.0.0.0 --port $PORT",
    "healthcheckPath": "/health",
    "healthcheckTimeout": 30
  }
}
